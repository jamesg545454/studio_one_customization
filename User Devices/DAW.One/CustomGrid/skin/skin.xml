<Skin>
  <!-- Resources -->
  <Resources>
    <Image name="buttonBack"	url="buttonBack.png"	frames="normal mouseover pressed"/>
  </Resources>
  <!-- Styles -->
  <Styles>
    <Style name="credits">
      <Font  name="textfont"   themeid="PresonusUI" size="10"/>
      <Color name="textcolor"  color="#aaaaaa" />
    </Style>
    <Style name="catLabel">
      <Font  name="textfont"   themeid="PresonusUI" size="10"/>
      <Color name="textcolor"  color="#d0d0d0" />
    </Style>
	<Style name="focusField">
	  <Font  name="textfont"   themeid="PresonusUI" size="10" style="bold"/>
      <Color name="textcolor"  color="#aaaaaa" />
      <Align name="textalign"	align="left"/>
    </Style>
    <Style name="buttonA">
      <Font  name="textfont"   themeid="PresonusUI" size="13"/>
      <Color name="textcolor"  color="#EBECEC" />
      <Color name="backcolor" color="#549acd" />
    </Style>
    <Style name="buttonB">
