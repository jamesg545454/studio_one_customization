  <!-- CustomGrid Editor -->
  <Forms>
    <Form name="CustomGrid" title="CustomGrid Editor" windowstyle="sizable restoresize restorepos" attach="all" datatarget="ControlSurface/Editor/ControlList">
      <using controller="ControlSurface/Editor">
        <View attach="all fitsize" size="0,0,0,0">
          <Horizontal attach="all" width="1014" spacing="0" margin="5">
            <Vertical attach="all" spacing="5" margin="0">

            <!-- Focus Display -->
              <TextBox name="focusDeviceFullName" style="focusField" width="587" options="fittext transparent" />
                
              <!-- buttonA -->
              
              <Horizontal attach="left right" spacing="5" margin="0">
                <foreach variable="$index" count="8">
                  <using controller="buttonA[$index]">
                    <TextBox name="paramTitle" style="buttonA" width="128" options="fittext composited nocontextmenu" attach="left right fill" height="69">
                      <Button name="controlValue" image="buttonBack" attach="all fill" />
                      <TextBox name="controlMessage" style="catLabel" options="transparent" size="5,49,0,0"/>
                    </TextBox>
                  </using>
                </foreach>
              </Horizontal>
              
